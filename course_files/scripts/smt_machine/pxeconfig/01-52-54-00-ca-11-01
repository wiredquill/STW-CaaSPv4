  kernel caasp/linux
  append load_ramdisk=1 initrd=caasp/initrd netsetup=dhcp hostname=worker11 autoyast=http://192.168.110.99/autoyast install=http://192.168.110.2/repo/SUSE/Install/SUSE-CAASP/x86_64

